#define _CRT_SECURE_NO_WARNINGS
#define MAX 200000
#include<iostream>
#include<queue>
#include<vector>
#include<memory.h>
#include<string.h>
#include<stdio.h>
#include<list>;
using namespace std;

list<string> answer;

int main() {
	string str;
	string tmp = "";
	int count = 0;
	char a;
	cin >> str;

	for (int i = 0; i < str.size(); i++) {
		if (str[i] != str[i+1]) {
			if (count == 0) {
				tmp += str[i];
				if (i == str.size()-1 ) {
					answer.push_back(tmp);
					break;
				}
			}
			else {

				//count>0이고 다른거 만난 상황으로 새로 저장해야함
				if (i == str.size() - 1) {
					answer.push_back("");
				}
				tmp = "";
				count = 0;
				continue;

			}
		}
		else {//같은 글자
			count++;

			//이전까지 쌓아온거 입력, 이후꺼도 입력
			
			if (count == 1) {
				answer.push_back(tmp);
			}
			
		}


	}
	int count2 = 0;
	printf("[");
	for (string val : answer) {
		if (val == "") {
			cout << "\"";
			cout << "\"";
			count2++;
		}
		else {
			cout << "\"";
			cout << val;
			cout << "\"";
			count2++;
		}
		if (count2 != answer.size()) {
			cout << ",";
		}
		

	}
	cout << "]";




}

