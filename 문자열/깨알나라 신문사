#include <iostream>

using namespace  std;

char arr[51][51];

int main() {
 	
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	
	int n, m, r, c;
	cin >> n >> m >> r >> c;

	for (int i = 0; i < n; i++) {
		for (int j = 0; j < m; j++) {
			cin >> arr[i][j];

		}
	}

	for (int i = 0; i < r; i++) {
		for (int j = 0; j < n; j++) {
			for (int s = 0; s < m; s++) {
				for (int e = 0; e < c; e++) {
					cout << arr[j][s];

				}

			}
			cout << '\n';
		}
		


	}
	
}
